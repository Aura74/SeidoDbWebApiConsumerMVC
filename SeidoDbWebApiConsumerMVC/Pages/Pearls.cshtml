@page
@model SeidoDbWebApiConsumerMVC.Pages.PearlsModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{ViewData["Title"] = "Pearl List";}

<div class="row">
    <table class="table">
        <thead class="thead-inverse">
            <tr><th>Necklaces</th></tr>
        </thead>
        <tbody>
                @foreach (var neck in Model.Pearls)
                {<tr><td>@neck</td></tr>}
        </tbody>
    </table>
</div>

